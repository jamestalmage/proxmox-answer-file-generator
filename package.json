		]
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^7.0.0",
		"ava": "^6.2.0",
		"execa": "^9.5.2",
		"temp-write": "^6.0.0",
		"tsimp": "^2.0.12",
		"typescript": "^5.7.3",
		"xo": "^0.60.0"
	}
}
